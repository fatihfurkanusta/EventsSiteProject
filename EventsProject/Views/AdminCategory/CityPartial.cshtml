@using EntityLayer.Concrete
@model List<City>

@foreach (var item in Model)
{

    <tr class="text-center">
        <td>
            @item.CityID
        </td>
        <td>
            @item.CityName

        </td>

        <td class="project-actions text-right">
           
            <a class="btn btn-danger btn-sm" href="/AdminCategory/DeleteCity/@item.CityID">
                <i class="fas fa-trash">
                </i>
                Sil
            </a>
        </td>
    </tr>
}
